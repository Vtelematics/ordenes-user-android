<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/co_s_bg_color"
    tools:context=".Fragments.CheckOutSuccess">

    <!--<ImageView
        android:id="@+id/img_co_s_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/image"
        android:scaleType="fitXY" />-->

    <FrameLayout
        android:id="@+id/lay_co_s_sub"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <!-- android:background="@color/transparent_whit_75"-->

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/_12sdp"
            android:layout_marginLeft="@dimen/_12sdp"
            android:layout_marginTop="@dimen/_12sdp"
            android:layout_marginEnd="@dimen/_12sdp"
            android:layout_marginRight="@dimen/_12sdp"
            android:layout_marginBottom="@dimen/_12sdp"
            android:background="@drawable/bg_for_layout_with_image">

            <TextView
                android:id="@+id/tv_co_s_msg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16sdp"
                android:layout_marginStart="@dimen/_12sdp"
                android:layout_marginLeft="@dimen/_12sdp"
                android:layout_marginEnd="@dimen/_12sdp"
                android:layout_marginRight="@dimen/_12sdp"
                android:fontFamily="@font/poppins_regular"
                android:gravity="center"
                android:text="@string/thank_you"
                android:padding="@dimen/_4sdp"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_13sdp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/lay_co_s_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_marginStart="@dimen/_12sdp"
                    android:layout_marginLeft="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_12sdp"
                    android:layout_marginRight="@dimen/_12sdp"
                    android:gravity="center"
                    android:lineSpacingExtra="@dimen/_3sdp"
                    android:padding="@dimen/_4sdp"
                    android:fontFamily="@font/poppins_semi_bold"
                    android:text="@string/co_s_order_placed_msg"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/_13sdp" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_4sdp"
                    android:layout_marginStart="@dimen/_36sdp"
                    android:layout_marginLeft="@dimen/_36sdp"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:layout_marginEnd="@dimen/_36sdp"
                    android:layout_marginRight="@dimen/_36sdp"
                    android:layout_marginBottom="@dimen/_4sdp"
                    android:layerType="software"
                    android:src="@drawable/line_restaurant_menu_grey_dotted" />

                <TextView
                    android:id="@+id/tv_co_s_thank_u"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_marginStart="@dimen/_12sdp"
                    android:layout_marginLeft="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_12sdp"
                    android:layout_marginRight="@dimen/_12sdp"
                    android:gravity="start"
                    android:lineSpacingExtra="@dimen/_3sdp"
                    android:paddingLeft="@dimen/_4sdp"
                    android:fontFamily="@font/poppins_regular"
                    android:text="@string/thank_you"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/_13sdp" />

                <!--<TextView
                    android:id="@+id/tv_co_s_thank_u_sub"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12sdp"
                    android:layout_marginLeft="@dimen/_12sdp"
                    android:layout_marginEnd="@dimen/_12sdp"
                    android:layout_marginRight="@dimen/_12sdp"
                    android:gravity="start"
                    android:padding="@dimen/_4sdp"
                    android:fontFamily="@font/poppins_regular"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/_13sdp" />-->

            </LinearLayout>

        </FrameLayout>


        <LinearLayout
            android:id="@+id/lay_co_s_continue"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_30sdp"
            android:layout_gravity="bottom"
            android:layout_marginStart="@dimen/_12sdp"
            android:layout_marginLeft="@dimen/_12sdp"
            android:layout_marginEnd="@dimen/_12sdp"
            android:layout_marginRight="@dimen/_12sdp"
            android:layout_marginBottom="@dimen/_12sdp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_co_s_continue"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_32sdp"
                android:fontFamily="@font/poppins_regular"
                android:background="@drawable/bg_new_req_btn_color_cart_checkout"
                android:gravity="center"
                android:text="@string/co_s_continue"
                android:textColor="@color/white_for_dark"
                android:textSize="@dimen/_14sdp"
                android:textStyle="bold" />

        </LinearLayout>

    </FrameLayout>




</FrameLayout>